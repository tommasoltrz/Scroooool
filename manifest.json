{
    "manifest_version": 2,

    "name": "Scroooll",
    "description": "Convert scrolled pixel to meters",
    "version": "1.0",
    "permissions": [
        "storage", "notifications"
    ],
    "content_scripts": [{
        "run_at": "document_end",
        "js": ["bg.js"],
        "matches": ["<all_urls>"]
    }],
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    }
}
